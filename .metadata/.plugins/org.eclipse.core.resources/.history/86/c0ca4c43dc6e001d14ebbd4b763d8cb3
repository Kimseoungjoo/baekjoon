public class baekjoon_10950 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		System.out.print(" ");
		int b = sc.nextInt();
		processMath(a,b);
	}
	public static void processMath(int a, int b) {
			System.out.println(a + b);
	}

}
