package baekjoon;

import java.util.Scanner;

public class baekjoon_10430 {

	public static void main(String[] args) {
		int a,b,c;
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("A 값 입력 >> ");
		a = sc.nextInt();
		
		System.out.println("B 값 입력 >> ");
		b = sc.nextInt();
		
		System.out.println("C 값 입력 >> ");
		c = sc.nextInt();
		
		
	}
	public void processMeth(int a, int b, int c) {
		System.out.println("%d \n %d \n %d \n %d",(a+b)%c,((a%c) + (b%c))%c,(a×b)%c,((a%c) × (b%c))%c );
	}

}
